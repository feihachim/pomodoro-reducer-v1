{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-uses-react": "off",
        "react/react-in-jsx-scope": "off",
        "linebreak-style": 0,
        "indent": ["error", 4],
        "react/jsx-indent":["error", 4],
        "import/no-extraneous-dependencies":["error", {"devDependencies": true}]
    }
}
